[
  {
    "key": "cmd+ctrl+[",
    "command": "moveActiveEditor",
    "args": {
      "to": "left",
      "by": "tab"
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+ctrl+]",
    "command": "moveActiveEditor",
    "args": {
      "to": "right",
      "by": "tab"
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+r",
    "command": "workbench.action.tasks.runTask"
  },
  {
    "key": "ctrl+cmd+left",
    "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "ctrl+cmd+up",
    "command": "workbench.action.moveEditorToAboveGroup"
  },
  {
    "key": "ctrl+cmd+down",
    "command": "workbench.action.moveEditorToBelowGroup"
  },
  {
    "key": "ctrl+cmd+left",
    "command": "workbench.action.moveEditorToLeftGroup"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "workbench.action.moveEditorToRightGroup"
  },
  {
    "key": "shift+cmd+c",
    "command": "-workbench.action.terminal.openNativeConsole"
  },
  {
    "key": "shift+cmd+c",
    "command": "workbench.action.createTerminalEditor"
  },
  {
    "key": "alt+cmd+/",
    "command": "editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+a",
    "command": "-editor.action.blockComment",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+[",
    "command": "-editor.action.inlineSuggest.showPrevious",
    "when": "!editorReadonly"
  },
  {
    "key": "ctrl+alt+cmd+up",
    "command": "workbench.action.increaseViewHeight"
  },
  {
    "key": "ctrl+alt+cmd+down",
    "command": "workbench.action.decreaseViewHeight"
  },
  {
    "key": "ctrl+cmd+\\",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "cmd+\\",
    "command": "-workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+1",
    "command": "workbench.action.terminal.focusAtIndex1"
  },
  {
    "key": "alt+cmd+2",
    "command": "workbench.action.terminal.focusAtIndex2"
  },
  // {
  //   "key": "shift+cmd+]",
  //   "command": "workbench.action.terminal.focusNextPane",
  //   "when": "terminalFocus && terminalProcessSupported"
  // },
  // {
  //   "key": "shift+cmd+[",
  //   "command": "workbench.action.terminal.focusPreviousPane",
  //   "when": "terminalFocus && terminalProcessSupported"
  // },
  // {
  //   "key": "alt+cmd+]",
  //   "command": "workbench.action.terminal.focusNext",
  //   "when": "terminalFocus && terminalProcessSupported"
  // },
  // {
  //   "key": "alt+cmd+[",
  //   "command": "workbench.action.terminal.focusPrevious",
  //   "when": "terminalFocus && terminalProcessSupported"
  // },
  {
    "key": "ctrl+alt+shift+right",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+alt+shift+left",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+alt+shift+up",
    "command": "workbench.action.focusAboveGroupWithoutWrap",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+alt+shift+right",
    "command": "workbench.action.focusRightGroupWithoutWrap",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+alt+shift+down",
    "command": "workbench.action.focusBelowGroupWithoutWrap",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+alt+shift+left",
    "command": "workbench.action.focusLeftGroupWithoutWrap",
    "when": "editorFocus"
  },
  {
    "key": "cmd+alt+up",
    "command": "workbench.action.focusAboveGroupWithoutWrap"
  },
  {
    "key": "cmd+alt+right",
    "command": "workbench.action.focusRightGroupWithoutWrap"
  },
  {
    "key": "cmd+alt+down",
    "command": "workbench.action.focusBelowGroupWithoutWrap"
  },
  {
    "key": "cmd+alt+left",
    "command": "workbench.action.focusLeftGroupWithoutWrap"
  },
  {
    "key": "cmd+0",
    "command": "workbench.action.focusLastEditorGroup"
  },
  // {
