{
  "cSpell.words": [
    "chsh",
    "dotfiles",
    "ghslp",
    "Heaslip",
    "jorgebucaran",
    "rushstack"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "files.associations": {
    "**/.vscode/settings.json": "jsonc",
    "**/vscode/keybindings.json": "jsonc",
    "**/vscode/settings.json": "jsonc"
  },
  "files.exclude": {
    ".yarn": true
  }
}
